function notypeparams(): integer
{
}

function onetypeparam<T>(value: T): T
{
	return value + value;
}

function twotypeparams<T1, T2>(): integer
{
}

/* Direct calls */

notypeparams();
onetypeparam<integer>(1);
onetypeparam<string>("1");
twotypeparams<integer, integer>();

/* Expression calls */

(notypeparams());
(onetypeparam<integer>(1));
(onetypeparam<string>("1"));
(twotypeparams<integer, integer>());
