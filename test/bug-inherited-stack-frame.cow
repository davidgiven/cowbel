type ListNode =
{
	function prev(): ListNode;
	function setPrev(n: ListNode); 
	function next(): ListNode;
	function setNext(n: ListNode);
};
	
function List()
{
	var head: ListNode =
	{
		implements ListNode;
		
		var _next = head;
		
		function setPrev(n: ListNode) {}
		function setNext(n: ListNode) _next = n;
	};
	
	var tail: ListNode =
	{
		implements ListNode;
		
		var _prev = head;
		
		function setPrev(n: ListNode) _prev = n;
		function setNext(n: ListNode) {}
	};
	
	tail.setPrev(head);
	head.setNext(tail);
}

List();
