#include "SimpleIO.ch"

for i = 1, 101
{
	if ((i % 15) == 0)
		println("Fizzbuzz");
	else if ((i % 3) == 0)
		println("Fizz");
	else if ((i % 5) == 0)
		println("Buzz");
	else
		println(i.toString());
}
