#include "SimpleIO.ch"
#include "Map.ch"

var map = Map<int, string>();

map.put(12, "twelve");
map.put(42, "forty-two");
map.put(99, "twelve");
map.put(-4, "minus four");

map.remove(99);
map.put(99, "ninety-nine");

println(map.get(-4));
println(map.get(42));
println(map.get(99));
println(map.contains(7).toString());
println(map.contains(12).toString());
